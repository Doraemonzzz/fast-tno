ninja_required_version = 1.3
cxx = /usr/bin/g++
nvcc = /usr/local/cuda-11.2/bin/nvcc

cflags = -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/data/qinzhen/code/fast-tno/src/fftconv/mathdx/22.02/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include/TH -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda-11.2/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/include/python3.8 -c
post_cflags = -g -march=native -funroll-loops -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=fftconv -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/data/qinzhen/code/fast-tno/src/fftconv/mathdx/22.02/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include/TH -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda-11.2/include -I/home/SENSETIME/qinzhen/anaconda3/envs/nlp/include/python3.8 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 --threads 4 -lineinfo --use_fast_math -std=c++17 -arch=compute_70 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=fftconv -D_GLIBCXX_USE_CXX11_ABI=0 -ccbin /usr/bin/gcc
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



build /data/qinzhen/code/fast-tno/src/fftconv/build/temp.linux-x86_64-3.8/fftconv.o: compile /data/qinzhen/code/fast-tno/src/fftconv/fftconv.cpp
build /data/qinzhen/code/fast-tno/src/fftconv/build/temp.linux-x86_64-3.8/fftconv_cuda.o: cuda_compile /data/qinzhen/code/fast-tno/src/fftconv/fftconv_cuda.cu





